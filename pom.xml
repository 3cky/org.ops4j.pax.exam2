<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">    <modelVersion>4.0.0</modelVersion>    <parent>        <groupId>org.ops4j</groupId>        <artifactId>master</artifactId>        <version>1.0.8</version>    </parent>    <groupId>org.ops4j.pax</groupId>    <artifactId>exam-reactor</artifactId>    <version>2.0.0-M2-SNAPSHOT</version>    <packaging>pom</packaging>    <name>OPS4J Pax Exam (Reactor POM)</name>    <profiles>        <profile>            <id>standard</id>            <modules>                <module>pom</module>                <!-- Fundamentals. Mostly API -->                <module>core/pax-exam</module>                <!-- Fundamental UI -->                <module>core/pax-exam-spi</module>                <!-- Low Level Communication -->                <module>core/pax-exam-extender</module>                <module>core/pax-exam-extender-service</module>                <!-- remoting glue (RMI impl, and possibly more (like http remoting) -->                <module>spice/pax-exam-container-rbc</module>                <module>spice/pax-exam-container-rbc-client</module>                <!-- Some containers -->                <module>containers/pax-exam-container-native</module>                <module>containers/pax-exam-container-remote</module>                <module>containers/pax-exam-container-paxrunner</module>                <!-- Brand new JUnit4 Driver -->                <module>drivers/pax-exam-junit4</module>                <!-- Maven plugin -->                <module>extensions/maven-paxexam-plugin</module>                <!-- Runtime Metadata Profiles for different Environments -->                <module>spice/pax-exam-link-mvn</module>                <module>spice/pax-exam-link-assembly</module>                <!-- non maven publications -->                <module>distributions/pax-exam-dist</module>                <module>distributions/pax-exam-uber</module>            </modules>        </profile>        <profile>            <!-- Just build the embedded "bundles" that are being installed dynamically. Others are covered by the IDE builder -->            <id>remotes</id>            <modules>                <module>core/pax-exam</module>                <module>core/pax-exam-extender</module>                <module>core/pax-exam-extender-service</module>                <module>spice/pax-exam-container-rbc</module>            </modules>        </profile>        <profile>            <id>exxam1</id>            <activation>                <activeByDefault>true</activeByDefault>            </activation>            <modules>                <!-- Fundamentals. Mostly API -->                <module>pom</module>                <module>core/pax-exam</module>                <module>core/pax-exam-spi</module>                <module>core/pax-exam-extender</module>                <module>core/pax-exam-extender-service</module>                <module>containers/pax-exam-container-native</module>                <module>spice/pax-exam-container-rbc</module>                <module>spice/pax-exam-container-rbc-client</module>                <module>containers/pax-exam-container-remote</module>                <module>drivers/pax-exam-junit4</module>                <module>spice/pax-exam-link-mvn</module>                <module>spice/pax-exam-link-assembly</module>                <module>it-regression/regression-native-base</module>                <module>it-regression/regression-native-felix</module>                <module>it-regression/regression-native-equinox</module>                                <module>it-regression/regression-native-multi</module>                                                <!--                <module>containers/pax-exam-container-paxrunner</module>                <module>it-regression/regression-paxrunner</module>                -->                            </modules>        </profile>        <profile>            <id>it</id>            <modules>                <module>samples/pax-exam-tutorial</module>                <module>samples/pax-exam-it-01</module>                <module>samples/probe1</module>                <module>samples/uberjar-test</module>                <module>samples/testing-container-paxrunner</module>            </modules>        </profile>    </profiles>    <scm>        <connection>scm:git:git@github.com:ops4j/org.ops4j.pax.exam2.git</connection>        <developerConnection>scm:git:git@github.com:ops4j/org.ops4j.pax.exam2.git</developerConnection>        <url>git@github.com:ops4j/org.ops4j.pax.exam2.git</url>    </scm>    <build>        <pluginManagement>            <plugins>                <plugin>                    <groupId>org.apache.maven.plugins</groupId>                    <artifactId>maven-release-plugin</artifactId>                    <version>2.1</version>                    <configuration>                        <preparationGoals>clean install</preparationGoals>                        <goals>deploy</goals>                        <autoVersionSubmodules>true</autoVersionSubmodules>                    </configuration>                </plugin>            </plugins>        </pluginManagement>    </build></project>